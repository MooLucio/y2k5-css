{
  "name": "y2k5-css",
  "version": "1.0.1",
  "description": "A CSS framework that brings back the look and feel of the 2005 web: XP blue gradients, solid borders, 11px Verdana, tables for layout, and pure y2k5 nostalgia.",
  "main": "dist/y2k5.css",
  "style": "dist/y2k5.css",
  "files": [
    "dist/",
    "themes/",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "y2k",
    "2005",
    "retro",
    "nostalgia",
    "geocities",
    "windows-xp",
    "web-2000s",
    "css",
    "framework",
    "aesthetic"
  ],
  "author": "MooLucio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MooLucio/y2k-css.git"
  },
  "bugs": {
    "url": "https://github.com/MooLucio/y2k-css/issues"
  },
  "homepage": "https://github.com/MooLucio/y2k5-css#readme",
  "scripts": {
    "clean": "rimraf dist",
    "copy": "xcopy src\\y2k5.css dist\\ /Y",
    "minify": "cleancss -o dist/y2k5.min.css src/y2k5.css",
    "build": "npm run clean && npm run copy && npm run minify",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "clean-css-cli": "^5.6.3",
    "rimraf": "^6.0.1"
  }
}
